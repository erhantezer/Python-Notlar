   ],
   "source": [
    "text = input()\n",
    "print(text)\n",
    "dict = {}\n",
    "for char in text:\n",
    "    if char in dict:\n",
    "        dict[char] += 1\n",
    "    else:\n",
    "        dict[char] = 1\n",
    "print(dict)"
